  <body>
    <tu>
      <tuv xml:lang="sms">
        <seg>Škoouʹlʼjem- da mättʼtemaaunâskoontâr</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>Koulutus- ja oppimateriaalitoimisto</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>Konttâr põõšši tuâjjla:</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>Toimiston vakinaiset työntekijät:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>Škoouʹlʼjempiizar</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>Koulutussihteeri</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>Vs. škoouʹlʼjempiizar Irmeli Moilanen 1.10.-31.12.2015</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>Irmeli Moilanen - toimiston esimies,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- škoouʹlʼjumuužž lääʹjjšiõttõõzz, vaaldšem, teäggtemooudâsviikkmõõžž, ärvvtõõllmõõžž, mättʼtemplaan-noormi da sääʹm-mättʼtõõzz ooudâsviikkâm praavmõš,</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- koulutuksen lainsäädännön, hallinnon, rahoituskehityksen, arvioinnin, opetussuunnitelmanormien ja saamelaisopetuksen kehityksen seuranta,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- škoouʹljʼem- da mättʼtemaaunâsluʹvddkååʹdd piizar da čiõʹlǧǧeei (škoouʹlʼjempolitiikklaž da takai škoouʹlʼjemääʹšš),</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- koulutus- ja oppimateriaalilautakunnan sihteeri ja esittelijä (koulutuspoliittiset ja yleiset koulutusasiat),</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- saaʹmi škoouʹlʼjemaaʹšši õhttsažtuâjjorgaan piizar da čiõʹlǧǧeei,</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- saamelaisten koulutusasiain yhteistyöelimen sihteeri ja esittelijä,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- õhttvuõđâânnmõš sääʹm-mättʹtõõzz riâšši kooʹddid, valdia mättʹtemvaaldšemma da tâʹvvjânnam vuäʹbborgaanid,</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- yhteydenpito saamelaisopetusta järjestäviin kuntiin, valtion opetushallintoon ja pohjoismaisiin sisarelimiin,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- meeraikõskksaž ooudâsviikkâm praavmõš.</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- kansainvälisen kehityksen seuranta.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>Mättʼtemaaunâspiizar</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>Oppimateriaalisihteeri</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- škoouʹlʼjem- da mättʼtemaaunâsluʹvddkååʹdd piizar da čiõʹlǧǧeei (mättʼtemaaunâsääʹšš),</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- koulutus- ja oppimateriaalilautakunnan sihteeri ja esittelijä (oppimateriaaliasiat),</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- mättʼtemaaunâstuâi plaanumuš, jååʹđtumuš, vuåppmõš da tuåimtemtuâjj,</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalityön suunnittelu, ohjaus, valvonta ja toimitustyö,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- mättʼtemaunnâz valmštumuuužž kuõskki õhttvuõttâânnmõš škoouʹlid da škoouʹlʼjem vaaldšemma,</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalin valmistukseen liittyvä yhteydenpito kouluihin ja opetuksen hallintoon,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>Näkkäläjärvi Anni</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- pohjoismainen yhteistyö.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- mättʼtemaunnâz plaaneem-, tuåimtem- da teâđtemtuâjj,</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalin suunnittelu-, toimitus- ja tiedotustyö,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- Sääʹmnuõri čeäʹppvuõttpeeiʹv valmštõõllmõš, teâđtumuš, čiõlǥtõs vaaʹri âânnmõõžžâst,</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- Saamelaisnuorten taidetapahtuman valmistelu, tiedotus, raportointi ja varainkäyttö,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- mättʼtemaunnâz valmštumuužž kuõskki õhttvuõttâânnmõš škoouʹlid da tâʹvvjânnamlaž õhttsažtuâjj,</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalin valmistukseen liittyvä yhteydenpito kouluihin ja pohjoismainen yhteistyö,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- mättʼtemaaunâsčuäʹjtõõzzâst da – seeiltempääiʹǩest huõl âânnmõš.</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalinäyttelyn ja – varaston ylläpito.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>Koontârpiizar</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>Toimistosihteeri</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- arkiivest da ǩeʹrjjpõõrtâst huõl âânnmõš,</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- arkiston ja kirjaston ylläpito,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- mättʼtemaaunâsvaaʹri âânnem praavmõš,</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalivarojen käytön seuranta,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- mättʼtemaunnâz teâđtemtuâjj, ruõkkmõš da juâkkmõš,</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- oppimateriaalin tiedotustyö, varastointi ja jakelu,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>- såbbri riâššâmnääl hoiʹddjumuš.</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>- kokousten käytännön järjestelyt.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>Elen Ánne Sara, tuõddârsää ʹ mǩiõll</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>Oppimateriaalin projektityöntekijät</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>Oona Länsman, tuõddârsää ʹ mǩiõll</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>Oona Länsman, pohjoissaame</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sms">
        <seg>Neeta Jääskö, aanarsääʹmǩiõll</seg>
      </tuv>
      <tuv xml:lang="fin">
        <seg>Neeta Jääskö, inarinsaame</seg>
      </tuv>
    </tu>
